@model List<LunchLibrary.Models.Log>

@{
    ViewData["Title"] = "Index";
}

<div class="row">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Create Time</th>
                <th scope="col">Message</th>
                <th scope="col">StackTrace</th>
            </tr>
        </thead>
        <tbody>
            @*<tr>
                    <th scope="row">1</th>
                    <td>Mark</td>
                    <td>Otto</td>
                    <td></td>
                </tr>*@
            @for (int i = 0; i < Model.Count; i++)
            {
                var ss = Model[i];
                <tr>
                    @{
                        <th scope="row">@(i + 1)</th>
                        <td>@ss.CreatedTime</td>
                        <td>@ss.Message</td>
                        <td>@ss.StackTrace</td>
                    }
                </tr>
            }
        </tbody>
    </table>
</div>